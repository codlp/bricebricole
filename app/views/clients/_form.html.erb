<%= simple_form_for @client do |f| %>
  <%= f.input :nom %>
  <%= f.input :prenom %>
  <%= f.input :telephone_1 %>
  <%= f.input :telephone_2 %>
  <%= f.input :email %>
  <%= f.input :adresse %>
  <%= f.input :categorie %>
  <%= f.input :information %>
  <%= f.input :fidelite %>
  <%= f.input :entreprise %>
  <%= f.submit "Ajouter", class: "btn btn-success mt-2" %>
<% end %>
